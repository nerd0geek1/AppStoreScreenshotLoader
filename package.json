{
  "name": "appstorescreenshotloader",
  "version": "0.1.0",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "AppStoreScreenshotLoader",
    "manifest": "src/manifest.json",
    "main": "appstorescreenshotloader.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.5.11"
  },
  "author": "Kohei Tabata <nerd0geek1@gmail.com>",
  "dependencies": {
    "skpm": "^1.1.11"
  }
}
